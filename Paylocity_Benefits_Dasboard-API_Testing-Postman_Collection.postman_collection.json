              "    pm.expect(jsonData.username).to.eql(\"TestUser891\");\r",
              "});\r",
              "\r",
              "// ==========================\r",
              "// VALIDATE REAL PERSISTENCE\r",
              "// ==========================\r",
              "pm.test(\"FirstName successfully persisted\", function () {\r",
              "    pm.expect(jsonData.firstName)\r",
              "      .to.eql(requestBody.firstName);\r",
              "});\r",
              "\r",
              "pm.test(\"LastName successfully persisted\", function () {\r",
              "    pm.expect(jsonData.lastName)\r",
              "      .to.eql(requestBody.lastName);\r",
              "});\r",
              "\r",
              "pm.test(\"Dependants persisted correctly\", function () {\r",
              "    pm.expect(jsonData.dependants)\r",
              "      .to.eql(requestBody.dependants);\r",
              "});\r",
              "\r",
              "// ========================================\r",
              "// 5️⃣ FINANCIAL VALIDATIONS (BY PAYCHECK)\r",
              "// ========================================\r",
              "\r",
              "// Business constants\r",
              "const paycheck = 2000;\r",
              "const paychecksPerYear = 26;\r",
              "const employeeBenefitYearly = 1000;\r",
              "const dependentBenefitYearly = 500;\r",
              "\r",
              "let dependants = jsonData.dependants;\r",
              "\r",
              "// Expected calculation\r",
              "let yearlyBenefits = employeeBenefitYearly + \r",
              "    (dependants * dependentBenefitYearly);\r",
              "\r",
              "let expectedBenefitsPerPaycheck = yearlyBenefits / paychecksPerYear;\r",
              "let expectedNetPerPaycheck = paycheck - expectedBenefitsPerPaycheck;\r",
              "\r",
              "// Validations\r",
              "pm.test(\"Gross correct\", function () {\r",
              "    pm.expect(jsonData.gross).to.eql(paycheck);\r",
              "});\r",
              "\r",
              "pm.test(\"Correct benefits\", function () {\r",
              "    pm.expect(jsonData.benefitsCost)\r",
              "      .to.be.closeTo(expectedBenefitsPerPaycheck, 0.01);\r",
              "});\r",
              "\r",
              "pm.test(\"Correct net\", function () {\r",
              "    pm.expect(jsonData.net)\r",
              "      .to.be.closeTo(expectedNetPerPaycheck, 0.01);\r",
              "});\r",
              "\r",
              "// ========================================\r",
              "// 6️⃣ INTEGRITY VALIDATIONS\r",
              "// ========================================\r",
              "pm.test(\"Net is always less than Gross\", function () {\r",
              "    pm.expect(jsonData.net).to.be.below(jsonData.gross);\r",
              "});\r",
              "\r",
              "pm.test(\"Benefits are never negative.\", function () {\r",
              "    pm.expect(jsonData.benefitsCost).to.be.at.least(0);\r",
              "});"
